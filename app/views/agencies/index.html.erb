<%# title 'Explore Agencies' %>
<% content_for :precolumn do %>
  <h1>Explore Agencies</h1>
<% end %>


<div class="header">
  <h2>By Name</h2>
</div>

<ul class="btn filter group">
  <li class="all"><a href="#">ALL</a></li>    
  <li><a href="#">A-C</a></li>
  <li><a href="#">D</a></li>
  <li><a href="#">E-F</a></li>
  <li><a href="#">G-N</a></li>
  <li><a href="#">O-Z</a></li>
</ul>

<ul class="agencyList">
<% @agencies.each do |agency|  %>
  <li><%= render :partial => 'agency_list_item', :locals => {:agency => agency, :chart_max => @chart_max} %></li>
<% end %>
</ul>

<% content_for :sidebar do %>
  <%= render :partial => 'agencies/featured', :locals => {:featured_agencies => @featured_agencies} %>  
<% end %>

