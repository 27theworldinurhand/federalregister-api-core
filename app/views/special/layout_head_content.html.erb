<link rel="Shortcut Icon" href="/favicon.ico">
<meta http-equiv="X-UA-Compatible" content="chrome=1">
<%# TODO HELP-RUBY for facebook image below, show curated image if present, should be 130wide max %>
<link rel="image_src" href="http://www.federalregister.gov/images/fbdisplay.jpg" />
<meta name="medium" content="news" />

<script type="text/javascript" src="https://use.typekit.com/qbb7wou.js"></script>
<script type="text/javascript">try{Typekit.load();}catch(e){}</script>

<%= yield :head %>

<%= stylesheet_link_tag "tipsy", :media => :screen %>
<%= stylesheet_link_tag "screen", :media => :screen %>
<%= stylesheet_link_tag "print", :media => :print %>


<!--[if IE 9]>
  <%= stylesheet_link_tag "ie9", :media => :screen %>  
<![endif]-->

<!--[if lt IE 9]>
  <%= stylesheet_link_tag "ie", :media => :screen %>  
<![endif]-->

<!--[if lte IE 7]>
  <%= stylesheet_link_tag "ie7", :media => :screen %>
<![endif]-->

<!--[if lt IE 7]>
  <%= stylesheet_link_tag "ie6", :media => :screen %>
<![endif]-->

<% if Airbrake.configuration.public? && ab_group == '2' %>
  <%= airbrake_javascript_notifier %>
  <script type="text/javascript">
  //<![CDATA[
  Airbrake.setErrorDefaults({ url: window.location });
  //]]>
  </script>
<% end %>

<% if Rails.env == 'development' || Rails.env == 'staging' %>
  <%= javascript_include_tag 'vendor/jquery-1.6.1.min.js' %>
<% else %>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.js" type="text/javascript"></script>
<% end %>
  
<script src="/javascripts/vendor/vendor.js" type="text/javascript"></script>
<script src="/javascripts/vendor/handlebars.1.0.0.beta.3.js" type="text/javascript"></script>
<script src="/javascripts/vendor/jquery.tipsy.js" type="text/javascript"></script>

<% if RAILS_ENV == 'development' || RAILS_ENV == 'staging' || (Airbrake.configuration.public? && ab_group == '2') %>
  <%= javascript_include_tag :all %>
<% else %>
  <%= javascript_include_tag 'all.js' %>
<% end %>

<%= render :partial => 'layouts/google_analytics', :custom_page_to_track => @page_to_track %>
