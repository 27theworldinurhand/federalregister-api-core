<div class="header_with_icon">
  <h1><%= @agency_highlight.section_header %></h1>
</div>

<div class="logo_and_title">
  <% if @agency_highlight.agency.logo_file_name.present? %>  
    <%= image_tag @agency_highlight.agency.logo.url(:thumb) %>
  <% end -%>
  <%= link_to @agency_highlight.agency.name, agency_path(@agency_highlight.agency) %>
</div>
<div class="content">

<h2><%= link_to @agency_highlight.title, entry_path(@agency_highlight.entry) %></h2>
<p><%= @agency_highlight.abstract %></p>
<%= link_to 'Read More', entry_path(@agency_highlight.entry) %>
</div>