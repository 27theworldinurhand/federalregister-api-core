<% title "Processing '#{@topic_name.name}'" %>

<% semantic_form_for @topic_name, :url => admin_topic_name_path(@topic_name) do |f| %>
  <% f.inputs do %>
    <%= f.input :topics, :as => :autocomplete, :source_data_url => "/admin/topics.json" %>
    <%= f.input :void %>
  <% end %>
  
  <% f.buttons do %>
    <%= f.commit_button %>
    <%= link_to 'Cancel', unprocessed_admin_topic_names_path %>
  <% end %>
<% end %>