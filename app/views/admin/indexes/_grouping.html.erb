<% if grouping.is_a?(FrIndexPresenter::SubjectGrouping) %>
  <%= render :partial => "subject_grouping", :locals => {
    :subject_grouping => grouping,
    :granule_class => granule_class
  } %>
<% elsif grouping.is_a?(FrIndexPresenter::DocumentGrouping) %>
  <%= render :partial => "document_grouping", :locals => {
    :document_grouping => grouping,
    :granule_class => granule_class
  } %>
<% end %>