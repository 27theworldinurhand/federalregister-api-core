<h4>FR Index Years</h4>

<ul>
  <% years.each do |year| %>
    <li>
      <%= link_to_unless_current  "#{year}", 
        admin_index_year_path(year, :max_date => (current_year == year ? params[:max_date] : nil)) %>
      <%= "<strong>&lt;&lt;</strong>".html_safe if current_year == year %>
    </li>
  <% end %>
</ul>
