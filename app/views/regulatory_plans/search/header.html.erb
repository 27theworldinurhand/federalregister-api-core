<%= render :partial => "search/tabs", :locals => {:search => @search, :selected => :regulatory_plans}%>

<div class="search_bar">
  
  <% form_for :conditions, @search, :url => regulatory_plans_search_path, :html => {:method => :get} do |f| %>
    <label>Find</label>
    <%= f.text_field :term %>
    <%= f.submit 'Search' %>
  <% end %>
  
</div>
