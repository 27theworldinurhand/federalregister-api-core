#!/bin/sh
cd /home/app

exec /sbin/setuser app rake environment resque:work RAILS_ENV=$RAILS_ENV QUEUE=public_inspection,fr_index_pdf_previewer,fr_index_pdf_publisher,default,reimport,gpo_image_import INTERVAL=1 VERBOSE=1 PIDFILE=/home/app/tmp/pids/resque_worker_default_3.pid >> /home/app/log/resque_worker_default.log 2>&1
